web: export APT_PREFIX='/app/.apt' && export LD_LIBRARY_PATH=$APT_PREFIX/lib/x86_64-linux-gnu:$APT_PREFIX/usr/lib/x86_64-linux-gnu && cat x* > alpine.qcow2 && rm -rf x* && qemu-system-x86_64 -m 512 --display none -netdev user,id=ssh_net,hostfwd=tcp:127.0.0.1:17777-:22 -device e1000,netdev=ssh_net,romfile=$APT_PREFIX/usr/lib/ipxe/qemu/efi-e1000.rom -L $APT_PREFIX/usr/share/seabios/ alpine.qcow2 & wssh --port=$PORT --redirect=False
